package com.greetings123;
 
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import java.time.Duration;
import org.openqa.selenium.chrome.ChromeDriver;


public class AdminTool {	

	public static void main(String[] args) {
		WebDriver driver = new ChromeDriver();
		driver.manage().window().maximize();
		driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(10));
		driver.get("http://db-master.123g.us/cgi-bin/admin_tools/super_admin_card_approval.pl");
		driver.findElement(By.xpath("/html/body/form[2]/table/tbody/tr[1]/th[1]/input")).click();
		driver.findElement(By.name("bt_accept")).click();
	//	driver.findElement(By.xpath("/html/body/form[1]/fieldset/div[1]/a")).click();
				
driver.close();
	}

}

